<?xml version="1.0" encoding="UTF-8"?><plugin xmlns="http://www.phonegap.com/ns/plugins/1.0"
	xmlns:android="http://schemas.android.com/apk/res/android"
	id="com.telerik.TestPlugin"
	version="1.0.0">

	<name>TestPlugin</name>

	<description>Performs TestPlugin.</description>

	<engines>
		<engine name="cordova" version=">=3.0.0" />
	</engines>

	<!-- android -->
	<platform name="android">
		<config-file target="res/xml/config.xml" parent="/*">
			<feature name="TestPlugin" >
				<param name="android-package" value="com.telerik.TestPlugin.TestPlugin"/>
				<param name="onload" value="true" />
			</feature>
		</config-file>

		<source-file src="src/android/TestPlugin.java" target-dir="src/com/telerik/TestPlugin" />
	</platform>
</plugin>
